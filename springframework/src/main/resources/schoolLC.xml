<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.springcore.lifecycle.School" name="school"  init-method="init" destroy-method="destroy" p:address="New Baneshwor" p:name="Big Boss" p:toalStudent="1200" />
    <bean class="org.springcore.lifecycle.Coder" name="coder" p:name="Prabesh" p:favouriteLanguage="Java" />

</beans>